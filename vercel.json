{
  "build": {
    "env": {
      "HUGO_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/musebox-dreams",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}

